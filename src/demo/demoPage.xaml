<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:demo" 
             x:Class="demo.demoPage">
    
	<StackLayout Padding="30,0">
        <Label Style="{StaticResource Header}" Text="A great Xamarin Demo App" />
        <Label Style="{StaticResource SubHeader}" Text="The best" />
        <Button x:Name="BehaviorButton" Text="Behaviors" />
        <Button x:Name="AnimationButton" Text="Animations" />
        <Button x:Name="lottieButton" Text="Lottie Animations" />
        <Button x:Name="EffectButton" Text="Effects" />
        <Button x:Name="EmbeddedButton" Text="Embedded Controls" />
    </StackLayout>
</ContentPage>
